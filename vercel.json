{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://worldapp.world"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://worldapp.world https://world.org"
        }
      ]
    }
  ]
}